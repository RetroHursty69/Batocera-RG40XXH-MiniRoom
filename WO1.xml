theme name:     MiniRoom RG40XXH
creator:		Dwayne Hurst
version         1
-->

<?xml version="1.0" encoding="UTF-8"?>

<theme>
   <formatVersion>7</formatVersion> 
   
<feature supported="carousel">
   <view name="system">
      <carousel name="systemcarousel">
        <type>vertical</type>
        <pos>0.26 0</pos>
        <logoAlignment>center</logoAlignment>         
        <size>1 1</size>
        <logoSize>0.32 0.32</logoSize>        
        <logoScale>1.05</logoScale>
        <zIndex>4</zIndex>          
        <color>00000000</color>
        <maxLogoCount>9</maxLogoCount>       
      </carousel>              

		<image name="logo">
		<linearSmooth>true</linearSmooth>
			<storyboard event="activate">
				<animation property="x" to="2" begin="2000" duration="500" mode="EaseIn"/>
			</storyboard>
			<storyboard event="scroll">
				<animation property="x" to="2" begin="2000" duration="500" mode="EaseIn"/>
			</storyboard>
			<storyboard event="deactivate">
				<animation property="x" to="2" begin="2000" duration="500" mode="EaseIn"/>
			</storyboard>
		</image>

       <image name="logo">
	     <path>./_inc/wheels1/${system.theme}.png</path>
       </image>           		
            
   </view>
</feature>
   
  
</theme>